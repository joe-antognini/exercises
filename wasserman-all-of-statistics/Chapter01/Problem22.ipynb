   "source": [
    "import numpy as np"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "np.random.seed(0)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [],
   "source": [
    "def compare_X(n, p=0.3):\n",
    "    X = np.random.binomial(n, p)\n",
    "    \n",
    "    print('n:', n)\n",
    "    print('np:', n * p)\n",
    "    print('X:', X)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "n: 10\n",
      "np: 3.0\n",
      "X: 3\n",
      "\n",
      "n: 100\n",
      "np: 30.0\n",
      "X: 33\n",
      "\n",
      "n: 1000\n",
      "np: 300.0\n",
      "X: 310\n",
      "\n"
     ]
    }
   ],
   "source": [
    "for n in [10, 100, 1000]:\n",
    "    compare_X(n)\n",
    "    print()"
   ]
  }
